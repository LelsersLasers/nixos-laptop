{
  "bar.customModules.storage.paths": [
    "/"
  ],
  "menus.power.lowBatteryNotification": true,
  "theme.bar.transparent": true,
  "theme.font.size": "12px",
  "terminal": "alacritty",
  "theme.font.weight": 600,
  "theme.bar.floating": false,
  "theme.bar.menus.menu.clock.scaling": 80,
  "menus.transitionTime": 0,
  "bar.layouts": {
    "*": {
      "left": [
        "workspaces",
        "media",
        "windowtitle"
      ],
      "middle": [
        "clock",
        "notifications"
      ],
      "right": [
        "systray",
        "cpu",
        "ram",
        "volume",
        "bluetooth",
        "network",
        "battery",
        "dashboard"
      ]
    }
  },
  "theme.bar.buttons.enableBorders": false,
  "bar.launcher.icon": "$",
  "bar.media.show_active_only": true,
  "bar.network.showWifiInfo": true,
  "bar.workspaces.show_icons": false,
  "bar.workspaces.show_numbered": true,
  "bar.launcher.autoDetectIcon": false,
  "theme.bar.buttons.workspaces.numbered_active_highlight_border": "0.2em",
  "theme.bar.buttons.workspaces.numbered_active_highlight_padding": "0.2em",
  "theme.bar.buttons.workspaces.numbered_inactive_padding": "0.2em",
  "theme.bar.buttons.notifications.spacing": "0.6em",
  "theme.bar.outer_spacing": "8px",
  "theme.bar.border_radius": "8px",
  "theme.bar.buttons.innerRadiusMultiplier": "0.4",
  "theme.bar.buttons.padding_x": "0.7rem",
  "theme.bar.buttons.volume.enableBorder": false,
  "bar.clock.format": "%a %b %d  %I:%M %p",
  "menus.clock.weather.enabled": false,
  "menus.dashboard.shortcuts.enabled": false,
  "menus.dashboard.directories.enabled": false,
  "bar.notifications.show_total": true,
  "theme.bar.buttons.modules.cpu.spacing": "0.6em",
  "bar.customModules.ram.labelType": "percentage",
  "bar.customModules.ram.round": true,
  "theme.bar.buttons.modules.ram.spacing": "0.6em",
  "bar.workspaces.numbered_active_indicator": "underline",
  "theme.bar.buttons.windowtitle.spacing": "0.9em",
  "theme.bar.buttons.media.spacing": "0.9em",
  "theme.bar.buttons.modules.power.spacing": "0.45em",
  "bar.customModules.power.icon": "$",
  "theme.bar.menus.popover.scaling": 80,
  "theme.bar.menus.menu.power.scaling": 90,
  "scalingPriority": "hyprland"
}